{
  "name": "younion-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "EXPO_NO_TELEMETRY=1 npx expo start --clear",
    "dev:lan": "EXPO_NO_TELEMETRY=1 expo start --lan",
    "dev:tunnel": "EXPO_NO_TELEMETRY=1 expo start --tunnel",
    "dev:prod": "EXPO_NO_TELEMETRY=1 expo start --no-dev --minify",
    "ios": "expo run:ios",
    "ios:device": "expo run:ios --device",
    "android": "expo run:android",
    "android:device": "expo run:android --device",
    "native:ios": "expo run:ios",
    "native:android": "expo run:android",
    "lint": "expo lint",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:e2e": "detox test",
    "test:e2e:ios": "detox test --configuration ios.sim.debug",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "web": "expo start --web",
    "build:web": "expo export --platform web"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@expo/webpack-config": "^19.0.1",
    "@hookform/resolvers": "^3.3.4",
    "@lucide/lab": "^0.1.2",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@sentry/react-native": "^6.20.0",
    "@shopify/flash-list": "1.7.6",
    "@shopify/react-native-skia": "^1.2.3",
    "@supabase/supabase-js": "^2.46.1",
    "@tamagui/animations-react-native": "^1.114.3",
    "@tamagui/config": "^1.114.3",
    "@tamagui/core": "^1.114.3",
    "@tamagui/font-inter": "^1.114.3",
    "@tamagui/lucide-icons": "^1.132.18",
    "@tamagui/shorthands": "^1.114.3",
    "@tamagui/themes": "^1.114.3",
    "@tamagui/toast": "^1.114.3",
    "@tanstack/query-async-storage-persister": "^5.85.3",
    "@tanstack/react-query": "^5.60.5",
    "@tanstack/react-query-persist-client": "^5.85.3",
    "expo": "^53.0.20",
    "expo-blur": "~14.1.5",
    "expo-build-properties": "^0.14.8",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-router": "^5.1.4",
    "expo-splash-screen": "~0.30.0",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.0",
    "expo-web-browser": "~14.2.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.53.2",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
