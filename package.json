    "@vitest/coverage-v8": "^2.1.4",
    "babel-plugin-module-resolver": "^5.0.2",
    "jsdom": "^25.0.1",
    "react-test-renderer": "18.3.1",
    "typescript": "~5.8.3",
    "vitest": "^3.2.4"
  }
